{
  "name": "mochiwallet",
  "private": true,
  "version": "0.0.15",
  "type": "module",
  "scripts": {
    "icons": "tsx scripts/create-icons.ts",
    "dev": "vite",
    "build": "tsc -b && vite build --mode production",
    "build:dev": "tsc -b && vite build --mode development",
    "watch": "tsc -b --watch & vite build --watch --mode development",
    "build:extension": "tsc -b && vite build --mode extension",
    "build:web": "vite build --mode web",
    "build:mobile": "npm run build:web && npx cap sync",
    "build:android": "npm run build:web && npx cap sync android && cd android && cross-env-shell \"if [ -f gradlew ]; then ./gradlew assembleDebug; else gradlew.bat assembleDebug; fi\"",
    "build:ios": "npm run build:web && npx cap sync ios && npx cap open ios",
    "build:all": "npm run build:extension && npm run build:mobile && npm run build:android && npm run build:ios",
    "lint": "eslint .",
    "preview": "vite preview --outDir dist/web",
    "server": "tsx server/index.ts",
    "server:dev": "tsx watch server/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@reduxjs/toolkit": "^2.2.1",
    "@shadcn/ui": "^0.0.4",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "emoji-picker-react": "^4.12.0",
    "express": "^4.18.3",
    "framer-motion": "^11.18.2",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.468.0",
    "mochimo-mesh-api-client": "github:NickP005/mochimo-mesh-api-client",
    "mochimo-wallet": "github:NickP005/mochimo-wallet",
    "mochimo-wots": "github:adequatesystems/mochimo-wots",
    "process": "^0.11.10",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.0",
    "redux-saga": "^1.3.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.1",
    "@capacitor/cli": "^7.4.1",
    "@capacitor/core": "^7.4.1",
    "@capacitor/ios": "^7.4.1",
    "@eslint/js": "^9.15.0",
    "@types/chrome": "^0.0.291",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/redux-saga": "^0.10.5",
    "@types/webextension-polyfill": "^0.12.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env-shell": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-pwa": "^1.0.1",
    "webextension-polyfill": "^0.12.0"
  }
}
